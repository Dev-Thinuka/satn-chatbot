{
  "name": "satn-chatbot-frontend",
  "version": "1.0.1",
  "private": true,
  "description": "Static HTML/CSS/JS chat widget for SA Thomson Nerys",
  "scripts": {
    "start": "serve ./src -l 5173",
    "start:open": "serve ./src -l 5173 -o",
    "clean": "node -e \"import('fs').then(fs=>{['node_modules','package-lock.json'].forEach(p=>fs.promises.rm(p,{recursive:true,force:true}))})\"",
    "reinstall": "npm run clean && npm i"
  },
  "dependencies": {
    "serve": "14.2.5"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=10 <11"
  }
}
